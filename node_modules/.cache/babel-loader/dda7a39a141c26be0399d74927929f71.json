AA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,IAAA;;AAEA,YAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA,gBAAA,EACA,IADA,CACA,YAAA;AACA,cAAA,MAAA,CAAA,MAAA,CAAA;AACA,gBAAA,SAAA,EAAA,qBADA;AAEA,gBAAA,QAAA,EAAA,WAFA;AAGA,gBAAA,KAAA,EAAA;AACA,kBAAA,KAAA,EAAA,QAAA,CAAA,IAAA,CAAA,OADA;AAEA,kBAAA,IAAA,EAAA,UAFA;AAGA,kBAAA,OAAA,EAAA;AAHA;AAHA,eAAA;AASA,aAXA;AAYA,WAfA,MAeA;AACA,YAAA,MAAA,CAAA,YAAA,GAAA,KAAA;;AAEA,YAAA,MAAA,CAAA,MAAA,CAAA;AACA,cAAA,SAAA,EAAA,qBADA;AAEA,cAAA,QAAA,EAAA,WAFA;AAGA,cAAA,KAAA,EAAA;AACA,gBAAA,KAAA,EAAA,QAAA,CAAA,IAAA,CAAA,OADA;AAEA,gBAAA,IAAA,EAAA,iBAFA;AAGA,gBAAA,OAAA,EAAA;AAHA;AAHA,aAAA;AASA;AACA,SA7BA,MA6BA;AACA,UAAA,MAAA,CAAA,YAAA,GAAA,KAAA;;AAEA,UAAA,MAAA,CAAA,MAAA,CAAA;AACA,YAAA,SAAA,EAAA,qBADA;AAEA,YAAA,QAAA,EAAA,WAFA;AAGA,YAAA,KAAA,EAAA;AACA,cAAA,KAAA,EAAA,OADA;AAEA,cAAA,IAAA,EAAA,iBAFA;AAGA,cAAA,OAAA,EAAA,QAHA;AAIA,cAAA,IAAA,EAAA;AAJA;AAHA,WAAA;AAUA;AACA,OA5CA,EA4CA,KA5CA,CA4CA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,KAAA;AACA,OA9CA,CAFA;AAiDA;AAvGA;AArCA,CAAA","sourcesContent":["<template>\n  <b-card>\n    <b-form @submit.prevent>\n      <b-row>\n\n        <b-col md=\"6\">\n          <b-form-group\n            label=\"Name\"\n            label-for=\"name\"\n          >\n\n            <b-input-group>\n\n              <b-form-input\n                id=\"name\"\n                v-model=\"formValues.name\"\n                type=\"text\"\n                placeholder=\"Item Name\"\n              />\n            </b-input-group>\n          </b-form-group>\n        </b-col>\n\n        <b-col md=\"6\">\n          <b-form-group\n            label=\"Name AR\"\n            label-for=\"name_ar\"\n          >\n\n            <b-input-group>\n\n              <b-form-input\n                id=\"name_ar\"\n                v-model=\"formValues.name_ar\"\n                type=\"text\"\n                placeholder=\"Item Name Arabic\"\n              />\n            </b-input-group>\n          </b-form-group>\n        </b-col>\n        <!-- submit and reset -->\n\n        <b-col md=\"12\">\n          <b-button\n            v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\n            type=\"submit\"\n            variant=\"primary\"\n            class=\"mr-1\"\n            :disabled=\"isSubmitting\"\n            @click=\"submitResource\"\n          >\n            <b-spinner\n              v-if=\"isSubmitting\"\n              small\n            />\n            <span v-if=\"isSubmitting\">Please Wait</span>\n\n            <span v-if=\"!isSubmitting\">Submit</span>\n          </b-button>\n\n        </b-col>\n\n      </b-row>\n    </b-form>\n  </b-card>\n</template>\n\n<script>\nimport {\n  BRow, BCol, BFormGroup, BFormInput, BFormCheckbox, BForm, BButton, BFormDatepicker, BCard, BFormFile, BSpinner, BInputGroup, BInputGroupPrepend, BFormCheckboxGroup, BFormTextarea,\n} from 'bootstrap-vue'\nimport Ripple from 'vue-ripple-directive'\nimport axios from '@axios'\nimport ToastificationContent from '@core/components/toastification/ToastificationContent.vue'\n\nexport default {\n  components: {\n    BRow,\n    BCol,\n    BFormGroup,\n    BFormInput,\n    BFormCheckbox,\n    BForm,\n    BButton,\n    BFormDatepicker,\n    BCard,\n    BFormFile,\n    BSpinner,\n    BInputGroup,\n    BInputGroupPrepend,\n    BFormCheckboxGroup,\n    BFormTextarea,\n  },\n  directives: {\n    Ripple,\n  },\n\n  data() {\n    return {\n      isSubmitting: false,\n      formValues: {\n        name: '',\n        name_ar: '',\n        id: '',\n      },\n\n    }\n  },\n  mounted() {\n    this.getItemDetail()\n  },\n\n  methods: {\n\n    getItemDetail() {\n      console.log(this.$route.params.id)\n\n      const config = {\n        headers: {\n          'content-type': 'multipart/form-data',\n\n        },\n      }\n\n      axios.get('/getExpenceItemDetail', {\n        params: {\n          id: this.$route.params.id,\n        },\n      }, config).then(response => {\n        if (response.data.hasOwnProperty('success')) {\n          if (response.data.success === true) {\n            this.formValues.name = response.data.data.name\n            this.formValues.name_ar = response.data.data.name_ar\n            console.log('done')\n          } else {\n            this.$toast({\n              component: ToastificationContent,\n              position: 'top-right',\n              props: {\n                title: 'Error',\n                icon: 'AlertCircleIcon',\n                variant: 'danger',\n                text: response.data.message,\n              },\n            })\n          }\n        } else {\n          this.$toast({\n            component: ToastificationContent,\n            position: 'top-right',\n            props: {\n              title: 'Error',\n              icon: 'AlertCircleIcon',\n              variant: 'danger',\n              text: 'Something went wrong, try again later',\n            },\n          })\n        }\n      }).catch(error => {\n        console.error(error)\n      })\n    },\n\n    submitResource() {\n      this.isSubmitting = true\n\n      this.formValues.id = this.$route.params.id,\n\n      axios.post('/updateExpenceItem', this.formValues).then(response => {\n        if (response.data.hasOwnProperty('success')) {\n          if (response.data.success === true) {\n            console.log(response.data.data)\n\n            this.$router.replace('/expence/items')\n              .then(() => {\n                this.$toast({\n                  component: ToastificationContent,\n                  position: 'top-right',\n                  props: {\n                    title: response.data.message,\n                    icon: 'EditIcon',\n                    variant: 'success',\n                  },\n                })\n              })\n          } else {\n            this.isSubmitting = false\n\n            this.$toast({\n              component: ToastificationContent,\n              position: 'top-right',\n              props: {\n                title: response.data.message,\n                icon: 'AlertCircleIcon',\n                variant: 'danger',\n              },\n            })\n          }\n        } else {\n          this.isSubmitting = false\n\n          this.$toast({\n            component: ToastificationContent,\n            position: 'top-right',\n            props: {\n              title: 'Error',\n              icon: 'AlertCircleIcon',\n              variant: 'danger',\n              text: 'Something went wrong, try again later',\n            },\n          })\n        }\n      }).catch(error => {\n        console.error(error)\n      })\n    },\n\n  },\n}\n</script>\n"],"sourceRoot":"src/views/table/bs-table/invoice"}]}