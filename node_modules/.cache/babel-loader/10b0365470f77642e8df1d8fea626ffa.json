AAA,OALA;AAMA,IAAA,MAAA,EAAA,MANA;AAOA,IAAA,MAAA,EAAA,MAPA;AAQA,IAAA,OAAA,EAAA,OARA;AASA,IAAA,KAAA,EAAA,KATA;AAUA,IAAA,MAAA,EAAA,MAVA;AAWA,IAAA,SAAA,EAAA,SAXA;AAYA,IAAA,aAAA,EAAA,aAZA;AAaA,IAAA,WAAA,EAAA,WAbA;AAcA,IAAA,QAAA,EAAA,QAdA;AAgBA,IAAA,OAAA,EAAA;AAhBA,GADA;AAmBA,EAAA,KAnBA,mBAmBA;AACA,QAAA,6BAAA,GAAA,aAAA,CADA,CAGA;;AACA,QAAA,CAAA,KAAA,CAAA,SAAA,CAAA,6BAAA,CAAA,EAAA,KAAA,CAAA,cAAA,CAAA,6BAAA,EAAA,kBAAA,EAJA,CAMA;;AACA,IAAA,WAAA,CAAA,YAAA;AACA,UAAA,KAAA,CAAA,SAAA,CAAA,6BAAA,CAAA,EAAA,KAAA,CAAA,gBAAA,CAAA,6BAAA;AACA,KAFA,CAAA;AAIA,QAAA,aAAA,GAAA,CACA,YADA,EAEA,OAFA,EAGA,MAHA,EAIA,iBAJA,EAKA,UALA,CAAA;;AAQA,2BAmBA,eAAA,EAnBA;AAAA,QACA,aADA,oBACA,aADA;AAAA,QAEA,YAFA,oBAEA,YAFA;AAAA,QAGA,OAHA,oBAGA,OAHA;AAAA,QAIA,WAJA,oBAIA,WAJA;AAAA,QAKA,aALA,oBAKA,aALA;AAAA,QAMA,QANA,oBAMA,QANA;AAAA,QAOA,cAPA,oBAOA,cAPA;AAAA,QAQA,WARA,oBAQA,WARA;AAAA,QASA,MATA,oBASA,MATA;AAAA,QAUA,aAVA,oBAUA,aAVA;AAAA,QAWA,mBAXA,oBAWA,mBAXA;AAAA,QAaA,YAbA,oBAaA,YAbA;AAAA,QAeA,WAfA,oBAeA,WAfA;AAAA,QAiBA,kCAjBA,oBAiBA,kCAjBA;AAAA,QAkBA,0BAlBA,oBAkBA,0BAlBA;;AAqBA,WAAA;AACA,MAAA,aAAA,EAAA,aADA;AAEA,MAAA,YAAA,EAAA,YAFA;AAGA,MAAA,OAAA,EAAA,OAHA;AAIA,MAAA,WAAA,EAAA,WAJA;AAKA,MAAA,aAAA,EAAA,aALA;AAMA,MAAA,QAAA,EAAA,QANA;AAOA,MAAA,cAAA,EAAA,cAPA;AAQA,MAAA,WAAA,EAAA,WARA;AASA,MAAA,MAAA,EAAA,MATA;AAUA,MAAA,aAAA,EAAA,aAVA;AAWA,MAAA,mBAAA,EAAA,mBAXA;AAaA,MAAA,YAAA,EAAA,YAbA;AAeA,MAAA,WAAA,EAAA,WAfA;AAiBA,MAAA,aAAA,EAAA,aAjBA;AAmBA,MAAA,UAAA,EAAA,UAnBA;AAoBA,MAAA,kCAAA,EAAA,kCApBA;AAqBA,MAAA,0BAAA,EAAA;AArBA,KAAA;AAuBA;AAlFA,CAAA","sourcesContent":["<template>\n\n  <!-- Table Container Card -->\n  <b-card\n    no-body\n  >\n\n    <div class=\"m-2\">\n\n      <!-- Table Top -->\n      <b-row>\n\n        <!-- Per Page -->\n        <b-col\n          cols=\"12\"\n          md=\"6\"\n          class=\"d-flex align-items-center justify-content-start mb-1 mb-md-0\"\n        >\n          <label>Entries</label>\n          <v-select\n            v-model=\"perPage\"\n            :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n            :options=\"perPageOptions\"\n            :clearable=\"false\"\n            class=\"per-page-selector d-inline-block ml-50 mr-1\"\n          />\n          <b-button\n            variant=\"primary\"\n            :to=\"{ name: 'apps-invoice-add'}\"\n          >\n            Add Record\n          </b-button>\n        </b-col>\n\n        <!-- Search -->\n        <b-col\n          cols=\"12\"\n          md=\"6\"\n        >\n          <div class=\"d-flex align-items-center justify-content-end\">\n            <b-form-input\n              v-model=\"searchQuery\"\n              class=\"d-inline-block mr-1\"\n              placeholder=\"Search...\"\n            />\n            <v-select\n              v-model=\"statusFilter\"\n              :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n              :options=\"statusOptions\"\n              class=\"invoice-filter-select\"\n              placeholder=\"Select Status\"\n            >\n              <template #selected-option=\"{ label }\">\n                <span class=\"text-truncate overflow-hidden\">\n                  {{ label }}\n                </span>\n              </template>\n            </v-select>\n          </div>\n        </b-col>\n      </b-row>\n\n    </div>\n\n    <b-table\n      ref=\"refInvoiceListTable\"\n      :items=\"fetchInvoices\"\n      responsive\n      :fields=\"tableColumns\"\n      primary-key=\"id\"\n      :sort-by.sync=\"sortBy\"\n      show-empty\n      empty-text=\"No matching records found\"\n      :sort-desc.sync=\"isSortDirDesc\"\n      class=\"position-relative\"\n    >\n\n      <template #head(invoiceStatus)>\n        <feather-icon\n          icon=\"TrendingUpIcon\"\n          class=\"mx-auto\"\n        />\n      </template>\n\n      <!-- Column: Id -->\n      <template #cell(id)=\"data\">\n        <b-link\n          :to=\"{ name: 'apps-invoice-preview', params: { id: data.item.id }}\"\n          class=\"font-weight-bold\"\n        >\n          #{{ data.value }}\n        </b-link>\n      </template>\n\n      <!-- Column: Invoice Status -->\n      <template #cell(invoiceStatus)=\"data\">\n        <b-avatar\n          :id=\"`invoice-row-${data.item.id}`\"\n          size=\"32\"\n          :variant=\"`light-${resolveInvoiceStatusVariantAndIcon(data.item.invoiceStatus).variant}`\"\n        >\n          <feather-icon\n            :icon=\"resolveInvoiceStatusVariantAndIcon(data.item.invoiceStatus).icon\"\n          />\n        </b-avatar>\n        <b-tooltip\n          :target=\"`invoice-row-${data.item.id}`\"\n          placement=\"top\"\n        >\n          <p class=\"mb-0\">\n            {{ data.item.invoiceStatus }}\n          </p>\n          <p class=\"mb-0\">\n            Balance: {{ data.item.balance }}\n          </p>\n          <p class=\"mb-0\">\n            Due Date: {{ data.item.dueDate }}\n          </p>\n        </b-tooltip>\n      </template>\n\n      <!-- Column: Client -->\n      <template #cell(client)=\"data\">\n        <b-media vertical-align=\"center\">\n          <template #aside>\n            <b-avatar\n              size=\"32\"\n              :src=\"data.item.avatar\"\n              :text=\"avatarText(data.item.client.name)\"\n              :variant=\"`light-${resolveClientAvatarVariant(data.item.invoiceStatus)}`\"\n            />\n          </template>\n          <span class=\"font-weight-bold d-block text-nowrap\">\n            {{ data.item.client.name }}\n          </span>\n          <small class=\"text-muted\">{{ data.item.client.companyEmail }}</small>\n        </b-media>\n      </template>\n\n      <!-- Column: Issued Date -->\n      <template #cell(issuedDate)=\"data\">\n        <span class=\"text-nowrap\">\n          {{ data.value }}\n        </span>\n      </template>\n\n      <!-- Column: Balance -->\n      <template #cell(balance)=\"data\">\n        <template v-if=\"data.value === 0\">\n          <b-badge\n            pill\n            variant=\"light-success\"\n          >\n            Paid\n          </b-badge>\n        </template>\n        <template v-else>\n          {{ data.value }}\n        </template>\n      </template>\n\n      <!-- Column: Actions -->\n      <template #cell(actions)=\"data\">\n\n        <div class=\"text-nowrap\">\n          <feather-icon\n            :id=\"`invoice-row-${data.item.id}-send-icon`\"\n            icon=\"SendIcon\"\n            class=\"cursor-pointer\"\n            size=\"16\"\n          />\n          <b-tooltip\n            title=\"Send Invoice\"\n            class=\"cursor-pointer\"\n            :target=\"`invoice-row-${data.item.id}-send-icon`\"\n          />\n\n          <feather-icon\n            :id=\"`invoice-row-${data.item.id}-preview-icon`\"\n            icon=\"EyeIcon\"\n            size=\"16\"\n            class=\"mx-1\"\n            @click=\"$router.push({ name: 'apps-invoice-preview', params: { id: data.item.id }})\"\n          />\n          <b-tooltip\n            title=\"Preview Invoice\"\n            :target=\"`invoice-row-${data.item.id}-preview-icon`\"\n          />\n\n          <!-- Dropdown -->\n          <b-dropdown\n            variant=\"link\"\n            toggle-class=\"p-0\"\n            no-caret\n            :right=\"$store.state.appConfig.isRTL\"\n          >\n\n            <template #button-content>\n              <feather-icon\n                icon=\"MoreVerticalIcon\"\n                size=\"16\"\n                class=\"align-middle text-body\"\n              />\n            </template>\n            <b-dropdown-item>\n              <feather-icon icon=\"DownloadIcon\" />\n              <span class=\"align-middle ml-50\">Download</span>\n            </b-dropdown-item>\n            <b-dropdown-item :to=\"{ name: 'apps-invoice-edit', params: { id: data.item.id } }\">\n              <feather-icon icon=\"EditIcon\" />\n              <span class=\"align-middle ml-50\">Edit</span>\n            </b-dropdown-item>\n            <b-dropdown-item>\n              <feather-icon icon=\"TrashIcon\" />\n              <span class=\"align-middle ml-50\">Delete</span>\n            </b-dropdown-item>\n            <b-dropdown-item>\n              <feather-icon icon=\"CopyIcon\" />\n              <span class=\"align-middle ml-50\">Duplicate</span>\n            </b-dropdown-item>\n          </b-dropdown>\n        </div>\n      </template>\n\n    </b-table>\n    <div class=\"mx-2 mb-2\">\n      <b-row>\n\n        <b-col\n          cols=\"12\"\n          sm=\"6\"\n          class=\"d-flex align-items-center justify-content-center justify-content-sm-start\"\n        >\n          <span class=\"text-muted\">Showing {{ dataMeta.from }} to {{ dataMeta.to }} of {{ dataMeta.of }} entries</span>\n        </b-col>\n        <!-- Pagination -->\n        <b-col\n          cols=\"12\"\n          sm=\"6\"\n          class=\"d-flex align-items-center justify-content-center justify-content-sm-end\"\n        >\n\n          <b-pagination\n            v-model=\"currentPage\"\n            :total-rows=\"totalInvoices\"\n            :per-page=\"perPage\"\n            first-number\n            last-number\n            class=\"mb-0 mt-1 mt-sm-0\"\n            prev-class=\"prev-item\"\n            next-class=\"next-item\"\n          >\n            <template #prev-text>\n              <feather-icon\n                icon=\"ChevronLeftIcon\"\n                size=\"18\"\n              />\n            </template>\n            <template #next-text>\n              <feather-icon\n                icon=\"ChevronRightIcon\"\n                size=\"18\"\n              />\n            </template>\n          </b-pagination>\n\n        </b-col>\n\n      </b-row>\n    </div>\n  </b-card>\n\n</template>\n\n<script>\nimport {\n  BCard, BRow, BCol, BFormInput, BButton, BTable, BMedia, BAvatar, BLink,\n  BBadge, BDropdown, BDropdownItem, BPagination, BTooltip,\n} from 'bootstrap-vue'\nimport { avatarText } from '@core/utils/filter'\nimport vSelect from 'vue-select'\nimport { onUnmounted } from '@vue/composition-api'\nimport store from '@/store'\nimport useInvoicesList from './useInvoiceList'\n\nimport invoiceStoreModule from '../invoiceStoreModule'\n\nexport default {\n  components: {\n    BCard,\n    BRow,\n    BCol,\n    BFormInput,\n    BButton,\n    BTable,\n    BMedia,\n    BAvatar,\n    BLink,\n    BBadge,\n    BDropdown,\n    BDropdownItem,\n    BPagination,\n    BTooltip,\n\n    vSelect,\n  },\n  setup() {\n    const INVOICE_APP_STORE_MODULE_NAME = 'app-invoice'\n\n    // Register module\n    if (!store.hasModule(INVOICE_APP_STORE_MODULE_NAME)) store.registerModule(INVOICE_APP_STORE_MODULE_NAME, invoiceStoreModule)\n\n    // UnRegister on leave\n    onUnmounted(() => {\n      if (store.hasModule(INVOICE_APP_STORE_MODULE_NAME)) store.unregisterModule(INVOICE_APP_STORE_MODULE_NAME)\n    })\n\n    const statusOptions = [\n      'Downloaded',\n      'Draft',\n      'Paid',\n      'Partial Payment',\n      'Past Due',\n    ]\n\n    const {\n      fetchInvoices,\n      tableColumns,\n      perPage,\n      currentPage,\n      totalInvoices,\n      dataMeta,\n      perPageOptions,\n      searchQuery,\n      sortBy,\n      isSortDirDesc,\n      refInvoiceListTable,\n\n      statusFilter,\n\n      refetchData,\n\n      resolveInvoiceStatusVariantAndIcon,\n      resolveClientAvatarVariant,\n    } = useInvoicesList()\n\n    return {\n      fetchInvoices,\n      tableColumns,\n      perPage,\n      currentPage,\n      totalInvoices,\n      dataMeta,\n      perPageOptions,\n      searchQuery,\n      sortBy,\n      isSortDirDesc,\n      refInvoiceListTable,\n\n      statusFilter,\n\n      refetchData,\n\n      statusOptions,\n\n      avatarText,\n      resolveInvoiceStatusVariantAndIcon,\n      resolveClientAvatarVariant,\n    }\n  },\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.per-page-selector {\n  width: 90px;\n}\n\n.invoice-filter-select {\n  min-width: 190px;\n\n  ::v-deep .vs__selected-options {\n    flex-wrap: nowrap;\n  }\n\n  ::v-deep .vs__selected {\n    width: 100px;\n  }\n}\n</style>\n\n<style lang=\"scss\">\n@import '@core/scss/vue/libs/vue-select.scss';\n</style>\n"],"sourceRoot":"src/views/apps/invoice/invoice-list"}]}