edElectricityAmount\"\n        >\n          <b-input-group>\n            <b-input-group-prepend is-text>\n              SR\n            </b-input-group-prepend>\n            <b-form-input\n              id=\"fixedElectricityAmount\"\n              v-model=\"formValues.fixedElectricityAmount\"\n              type=\"text\"\n              placeholder=\"Fixed Electricity Amount\"\n            />\n          </b-input-group>\n\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Fixed Water Amount\"\n          label-for=\"fixedWaterAmount\"\n        >\n          <b-input-group>\n            <b-input-group-prepend is-text>\n              SR\n            </b-input-group-prepend>\n            <b-form-input\n              id=\"fixedWaterAmount\"\n              v-model=\"formValues.fixedWaterAmount\"\n              type=\"text\"\n              placeholder=\"Fixed Water Amount\"\n            />\n          </b-input-group>\n\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Contact Name\"\n          label-for=\"contactName\"\n        >\n          <b-form-input\n            id=\"contactName\"\n            v-model=\"formValues.contactName\"\n            type=\"text\"\n            placeholder=\"Contact Number\"\n          />\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Contact Number\"\n          label-for=\"contactNo\"\n        >\n          <cleave\n            id=\"contactNo\"\n            v-model=\"formValues.contactNo\"\n            class=\"form-control\"\n            :raw=\"false\"\n            :options=\"options.phone\"\n            placeholder=\"Contact Number\"\n          />\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Account Name\"\n          label-for=\"accountName\"\n        >\n          <b-form-input\n            id=\"accountName\