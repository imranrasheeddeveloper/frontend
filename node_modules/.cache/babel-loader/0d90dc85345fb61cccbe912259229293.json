       <b-form-group\n          label=\"Beds\"\n          label-for=\"beds\"\n        >\n          <b-form-input\n            id=\"beds\"\n            v-model=\"formValues.beds\"\n            type=\"number\"\n            placeholder=\"No of Beds\"\n          />\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"6\">\n\n        <label for=\"contract_startdate\">Contract Start Date</label>\n        <b-input-group>\n\n          <cleave\n            id=\"contract_startdate\"\n            v-model=\"formValues.contract_startdate\"\n            class=\"form-control\"\n            :raw=\"false\"\n            :options=\"options.date\"\n            placeholder=\"YYYY-MM-DD\"\n          />\n\n          <b-input-group-append>\n            <b-form-datepicker\n              v-model=\"formValues.contract_startdate\"\n              show-decade-nav\n              button-only\n              right\n              locale=\"en-US\"\n              aria-controls=\"contract_startdate\"\n            />\n          </b-input-group-append>\n\n        </b-input-group>\n      </b-col>\n\n      <b-col md=\"6\">\n\n        <label for=\"contract_enddate\">Contract End Date</label>\n        <b-input-group>\n\n          <cleave\n            id=\"contract_enddate\"\n            v-model=\"formValues.contract_enddate\"\n            class=\"form-control\"\n            :raw=\"false\"\n            :options=\"options.date\"\n            placeholder=\"YYYY-MM-DD\"\n          />\n\n          <b-input-group-append>\n            <b-form-datepicker\n              v-model=\"formValues.contract_enddate\"\n              show-decade-nav\n              button-only\n              right\n              locale=\"en-US\"\n              aria-controls=\"contract_enddate\"\n            />\n          </b-input-group-append>\n\n        </b-input-group>\n      </b-col>\n\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Apartment\"\n          label-for=\"apartment\"\n        >\n          <b-form-input\n            id=\"apartment\"\n            v-model=\"formValues.apartment\"\n            type=\"text\"\n            placeholder=\"Appartments\"\n          />\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Rent\"\n          label-for=\"rent\"\n        >\n          <b-input-group>\n            <b-input-group-prepend is-text>\n              SR\n            </b-input-group-prepend>\n            <b-form-input\n              id=\"rent\"\n              v-model=\"formValues.rent\"\n              type=\"text\"\n              placeholder=\"Accommodation rent\"\n            />\n          </b-input-group>\n\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Period\"\n          label-for=\"period\"\n        >\n          <v-select\n            id=\"period\"\n            v-model=\"formValues.period\"\n            :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n            :options=\"periodDropdown\"\n\n            label=\"text\"\n            :reduce=\"text => text.value\"\n          />\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"6\">\n\n        <label for=\"due-date\">Rent Due Date</label>\n        <b-input-group>\n\n          <cleave\n            id=\"due-date\"\n            v-model=\"formValues.dueDate\"\n            class=\"form-control\"\n            :raw=\"false\"\n            :options=\"options.date\"\n            placeholder=\"YYYY-MM-DD\"\n          />\n\n          <b-input-group-append>\n            <b-form-datepicker\n              v-model=\"formValues.dueDate\"\n              show-decade-nav\n              button-only\n              right\n              locale=\"en-US\"\n              aria-controls=\"due-date\"\n            />\n          </b-input-group-append>\n\n        </b-input-group>\n\n      </b-col>\n\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Services\"\n          label-for=\"services\"\n        >