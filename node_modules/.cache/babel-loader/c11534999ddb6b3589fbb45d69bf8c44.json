mposition Function\nimport useVerticalNavMenuGroup from './useVerticalNavMenuGroup'\nimport mixinVerticalNavMenuGroup from './mixinVerticalNavMenuGroup'\n\nexport default {\n  name: 'VerticalNavMenuGroup',\n  components: {\n    VerticalNavMenuHeader,\n    VerticalNavMenuLink,\n    BLink,\n    BBadge,\n    BCollapse,\n  },\n  mixins: [mixinVerticalNavMenuGroup],\n  props: {\n    item: {\n      type: Object,\n      required: true,\n    },\n  },\n  setup(props) {\n    const {\n      isOpen,\n      isActive,\n      updateGroupOpen,\n      updateIsActive,\n    } = useVerticalNavMenuGroup(props.item)\n\n    const { t } = useI18nUtils()\n    const { canViewVerticalNavMenuGroup } = useAclUtils()\n\n    return {\n      resolveNavItemComponent,\n      isOpen,\n      isActive,\n      updateGroupOpen,\n      updateIsActive,\n\n      // ACL\n      canViewVerticalNavMenuGroup,\n\n      // i18n\n      t,\n    }\n  },\n}\n</script>\n\n<style>\n\n</style>\n"],"sourceRoot":"src/@core/layouts/layout-vertical/components/vertical-nav-menu/components/vertical-nav-menu-group"}]}