YAAA,GAAA,KAAA;;AAEA,gBAAA,MAAA,CAAA,MAAA,CAAA;AACA,kBAAA,SAAA,EAAA,qBADA;AAEA,kBAAA,QAAA,EAAA,WAFA;AAGA,kBAAA,KAAA,EAAA;AACA,oBAAA,KAAA,EAAA,OADA;AAEA,oBAAA,IAAA,EAAA,iBAFA;AAGA,oBAAA,OAAA,EAAA,QAHA;AAIA,oBAAA,IAAA,EAAA;AAJA;AAHA,iBAAA;AAUA;AACA,aA5CA,EA4CA,KA5CA,CA4CA,UAAA,KAAA,EAAA;AACA,cAAA,OAAA,CAAA,KAAA,CAAA,KAAA;AACA,aA9CA,EAhBA,CA+DA;AACA,WAhEA,MAgEA;AACA,YAAA,MAAA,CAAA,YAAA,GAAA,KAAA;;AAEA,YAAA,MAAA,CAAA,MAAA,CAAA;AACA,cAAA,SAAA,EAAA,qBADA;AAEA,cAAA,QAAA,EAAA,WAFA;AAGA,cAAA,KAAA,EAAA;AACA,gBAAA,KAAA,EAAA,OADA;AAEA,gBAAA,IAAA,EAAA,iBAFA;AAGA,gBAAA,OAAA,EAAA,QAHA;AAIA,gBAAA,IAAA,EAAA,QAAA,CAAA,IAAA,CAAA;AAJA;AAHA,aAAA;AAUA;AACA,SA/EA,MA+EA;AACA,UAAA,MAAA,CAAA,YAAA,GAAA,KAAA;;AAEA,UAAA,MAAA,CAAA,MAAA,CAAA;AACA,YAAA,SAAA,EAAA,qBADA;AAEA,YAAA,QAAA,EAAA,WAFA;AAGA,YAAA,KAAA,EAAA;AACA,cAAA,KAAA,EAAA,OADA;AAEA,cAAA,IAAA,EAAA,iBAFA;AAGA,cAAA,OAAA,EAAA,QAHA;AAIA,cAAA,IAAA,EAAA;AAJA;AAHA,WAAA;AAUA;AACA,OAhGA,EAgGA,KAhGA,CAgGA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,KAAA;AACA,OAlGA;AAmGA;AAtNA;AA3FA,CAAA","sourcesContent":["<template>\n  <b-form @submit.prevent>\n    <b-row>\n\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Accommodation\"\n          label-for=\"accommodation\"\n        >\n\n          <vue-autosuggest\n          :suggestions=\"filteredOptions\"\n            :limit=\"10\"\n            :input-props=\"{id:'autosuggest__input',class:'form-control', placeholder:'Search with name or ID'}\"\n            @input=\"onInputChange\"\n\n            @selected=\"assignOwnerField\">\n            <template slot-scope=\"{suggestion}\">\n               <span class=\"my-suggestion-item\">{{ suggestion.item.name }}, {{ suggestion.item.id }} </span>\n            </template>\n          </vue-autosuggest>\n\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Amount\"\n          label-for=\"amount\"\n        >\n\n          <b-input-group>\n            <b-input-group-prepend is-text>\n              SR\n            </b-input-group-prepend>\n            <b-form-input\n              id=\"location\"\n              v-model=\"formValues.amount\"\n              type=\"text\"\n              placeholder=\"Amount payed\"\n            />\n          </b-input-group>\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Bill Period\"\n          label-for=\"periodFrom\"\n        >\n          <!-- <b-form-datepicker\n              id=\"periodFrom\"\n              v-model=\"formValues.billMonth\"\n              class=\"mb-1\"\n            /> -->\n          <vue-monthly-picker\n            v-model=\"formValues.billMonth\"\n            :month-labels=\"monthLabelArray\"\n          />\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Bill Type\"\n          label-for=\"periodTo\"\n        >\n\n          <v-select\n            id=\"periodTo\"\n            v-model=\"formValues.billType\"\n            :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n            :options=\"billTypes\"\n\n            label=\"text\"\n            :reduce=\"text => text.value\"\n          />\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"6\">\n\n        <label for=\"date\">Date of payment</label>\n        <b-input-group>\n\n          <cleave\n            id=\"date\"\n            v-model=\"formValues.date\"\n            class=\"form-control\"\n            :raw=\"false\"\n            :options=\"options.date\"\n            placeholder=\"YYYY-MM-DD\"\n          />\n\n          <b-input-group-append>\n            <b-form-datepicker\n              v-model=\"formValues.date\"\n              show-decade-nav\n              button-only\n              right\n              locale=\"en-US\"\n              aria-controls=\"date\"\n            />\n          </b-input-group-append>\n\n        </b-input-group>\n      </b-col>\n\n      <b-col md=\"6\">\n        <b-form-group\n          label=\"Attachment\"\n          label-for=\"v-purchaseOrder\"\n        >\n\n          <b-form-file\n            id=\"v-purchaseOrder\"\n            v-model=\"purchaseOrderFile\"\n            accept=\"image/* , .pdf\"\n            placeholder=\"Upload attachment...\"\n            drop-placeholder=\"Drop file here...\"\n          />\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"12\">\n        <b-form-group\n          label=\"Notes\"\n          label-for=\"notes\"\n        >\n          <b-form-textarea\n            id=\"notes\"\n            v-model=\"formValues.notes\"\n            placeholder=\"Notes\"\n            rows=\"3\"\n          />\n        </b-form-group>\n      </b-col>\n\n      <!-- submit and reset -->\n\n      <b-col md=\"12\">\n        <b-button\n          v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\n          type=\"submit\"\n          variant=\"primary\"\n          class=\"mr-1\"\n          :disabled=\"isSubmitting\"\n          @click=\"submitResource\"\n        >\n          <b-spinner\n            v-if=\"isSubmitting\"\n            small\n          />\n          <span v-if=\"isSubmitting\">Please Wait</span>\n\n          <span v-if=\"!isSubmitting\">Submit</span>\n        </b-button>\n\n      </b-col>\n\n    </b-row>\n  </b-form>\n</template>\n\n<script>\nimport {\n  BRow, BCol, BFormGroup, BFormInput, BFormCheckbox, BForm, BButton, BFormDatepicker, BCard, BFormFile, BSpinner, BInputGroup, BInputGroupPrepend, BFormCheckboxGroup, BFormTextarea, BInputGroupAppend,\n} from 'bootstrap-vue'\nimport Ripple from 'vue-ripple-directive'\nimport vSelect from 'vue-select'\nimport VueMonthlyPicker from 'vue-monthly-picker'\nimport { MonthPickerInput } from 'vue-month-picker'\nimport { VueAutosuggest } from 'vue-autosuggest'\nimport axios from '@axios'\nimport { heightTransition } from '@core/mixins/ui/transition'\nimport ToastificationContent from '@core/components/toastification/ToastificationContent.vue'\n\nimport Cleave from 'vue-cleave-component'\nimport 'cleave.js/dist/addons/cleave-phone.us'\n\nexport default {\n  components: {\n    BRow,\n    BCol,\n    BFormGroup,\n    BFormInput,\n    BFormCheckbox,\n    BForm,\n    BButton,\n    BFormDatepicker,\n    vSelect,\n    VueAutosuggest,\n    BCard,\n    BFormFile,\n    BSpinner,\n    BInputGroup,\n    BInputGroupPrepend,\n    BFormCheckboxGroup,\n    BFormTextarea,\n    VueMonthlyPicker,\n    MonthPickerInput,\n    BInputGroupAppend,\n    Cleave,\n  },\n  directives: {\n    Ripple,\n  },\n\n  mixins: [heightTransition],\n\n  data() {\n    return {\n      options: {\n\n        phone: {\n          prefix: '9665',\n          blocks: [12],\n          uppercase: true,\n          noImmediatePrefix: true,\n        },\n        date: {\n          date: true,\n          delimiter: '-',\n          datePattern: ['Y', 'm', 'd'],\n        },\n        account_number: {\n          blocks: [15],\n          uppercase: true,\n        },\n        iqama_number: {\n          blocks: [10],\n          uppercase: true,\n        },\n        cr_number: {\n          blocks: [10],\n          uppercase: true,\n        },\n        iban: {\n          prefix: 'SA',\n          blocks: [24],\n          uppercase: true,\n          noImmediatePrefix: true,\n        },\n\n      },\n      isSubmitting: false,\n      monthLabelArray: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],\n      billTypes: [\n        { value: 'Electricity', text: 'Electricity' },\n        { value: 'Water', text: 'Water' },\n        { value: 'Others', text: 'Others' },\n      ],\n      filteredOptions: [],\n      formValues: {\n        amount: '',\n        accommodation_base_id: '',\n        billType: '',\n        billMonth: '',\n        notes: '',\n        date: '',\n        attachment: '',\n      },\n\n      purchaseOrderFile: null,\n\n    }\n  },\n  mounted() {\n\n  },\n\n  methods: {\n\n    assignOwnerField(item) {\n      if (item) {\n        this.formValues.accommodation_base_id = item.item.id\n        console.log(this.allocationDate)\n      }\n    },\n\n    onInputChange(text) {\n      if (text === '' || text === undefined) {\n        return\n      }\n\n      axios.get('/getEmployeeDropdown', {\n        params: {\n          id: text,\n          resourceType: 'Accommodation',\n        },\n      }).then(response => {\n        if (response.data.hasOwnProperty('success')) {\n          if (response.data.success === true) {\n            console.log(response.data.data)\n            this.filteredOptions = [{ data: response.data.data }]\n\n            console.log('Accommodations Fetched')\n          } else {\n            this.$toast({\n              component: ToastificationContent,\n              position: 'top-right',\n              props: {\n                title: 'Error',\n                icon: 'AlertCircleIcon',\n                variant: 'danger',\n                text: response.data.message,\n              },\n            })\n          }\n        } else {\n          this.$toast({\n            component: ToastificationContent,\n            position: 'top-right',\n            props: {\n              title: 'Error',\n              icon: 'AlertCircleIcon',\n              variant: 'danger',\n              text: 'Something went wrong, try again later',\n            },\n          })\n        }\n      }).catch(error => {\n        console.error(error)\n      })\n    },\n\n    submitResource() {\n      this.isSubmitting = true\n\n      if (this.purchaseOrderFile != null) {\n        this.uploadImg(this.purchaseOrderFile)\n      } else {\n        axios.post('/addAccommodationBillPayment', this.formValues).then(response => {\n          if (response.data.hasOwnProperty('success')) {\n            if (response.data.success === true) {\n              console.log(response.data.data)\n\n              this.$router.replace('/accommodations')\n                .then(() => {\n                  this.$toast({\n                    component: ToastificationContent,\n      