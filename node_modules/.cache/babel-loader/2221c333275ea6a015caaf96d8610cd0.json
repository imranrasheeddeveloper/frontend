A,CAAA,EAAA;AACA,cAAA,QAAA,CAAA,IAAA,CAAA,OAAA,KAAA,IAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,IAAA;;AAEA,YAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA,mBAAA,EAAA,IAAA,CAAA,YAAA;AACA,cAAA,KAAA,CAAA,MAAA,CAAA;AACA,gBAAA,SAAA,EAAA,qBADA;AAEA,gBAAA,QAAA,EAAA,WAFA;AAGA,gBAAA,KAAA,EAAA;AACA,kBAAA,KAAA,EAAA,QAAA,CAAA,IAAA,CAAA,OADA;AAEA,kBAAA,IAAA,EAAA,UAFA;AAGA,kBAAA,OAAA,EAAA;AAHA;AAHA,eAAA;AASA,aAVA;AAWA,WAdA,MAcA;AACA,YAAA,KAAA,CAAA,YAAA,GAAA,KAAA;;AAEA,YAAA,KAAA,CAAA,MAAA,CAAA;AACA,cAAA,SAAA,EAAA,qBADA;AAEA,cAAA,QAAA,EAAA,WAFA;AAGA,cAAA,KAAA,EAAA;AACA,gBAAA,KAAA,EAAA,QAAA,CAAA,IAAA,CAAA,OADA;AAEA,gBAAA,IAAA,EAAA,iBAFA;AAGA,gBAAA,OAAA,EAAA;AAHA;AAHA,aAAA;AASA;AACA,SA5BA,MA4BA;AACA,UAAA,KAAA,CAAA,YAAA,GAAA,KAAA;;AAEA,UAAA,KAAA,CAAA,MAAA,CAAA;AACA,YAAA,SAAA,EAAA,qBADA;AAEA,YAAA,QAAA,EAAA,WAFA;AAGA,YAAA,KAAA,EAAA;AACA,cAAA,KAAA,EAAA,OADA;AAEA,cAAA,IAAA,EAAA,iBAFA;AAGA,cAAA,OAAA,EAAA,QAHA;AAIA,cAAA,IAAA,EAAA;AAJA;AAHA,WAAA;AAUA;AACA,OA7CA,EA8CA,KA9CA,CA8CA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,KAAA;AACA,OAhDA;AAiDA,KAhFA;AAiFA,IAAA,gBAjFA,4BAiFA,KAjFA,EAiFA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EAAA,KAAA,CAAA,IAAA;AACA,WAAA,UAAA,CAAA,IAAA,GAAA,KAAA,CAAA,IAAA,CAAA,IAAA;AACA,WAAA,UAAA,CAAA,OAAA,GAAA,KAAA,CAAA,IAAA,CAAA,EAAA;AAEA;AACA,KAvFA;AAwFA,IAAA,YAxFA,0BAwFA;AACA,WAAA,WAAA,CAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA,CACA;AACA,OAFA;AAGA;AA7FA;AA/DA,CAAA","sourcesContent":["<template>\n  <b-form @submit.prevent>\n    <b-row>\n\n      <b-col md=\"6\">\n        <b-form-group label=\"Name\" label-for=\"v-name\">\n          <b-form-input id=\"v-name'\" v-model=\"formValues.name\" />\n        </b-form-group>\n      </b-col>\n\n      <b-col md=\"6\">\n        <b-form-group label=\"Type\" label-for=\"v-type\">\n           <v-select\n            id=\"v-Status\"\n            v-model=\"formValues.type\"\n            :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n            :options=\"RequestType\"\n            label=\"text\"\n            :reduce=\"(text) => text.value\"\n        \n          />\n        </b-form-group>\n        </b-col>\n     \n      \n      <b-col md=\"12\">\n        <b-form-group label=\"Issue\" label-for=\"Issue\">\n          <b-form-textarea\n            id=\"Issue\"\n            v-model=\"formValues.issue\"\n            placeholder=\"Write Issue Here..\"\n            rows=\"3\"\n          />\n        </b-form-group>\n      </b-col>\n      \n\n      <!-- submit and reset -->\n      <b-col md=\"12\">\n        <b-button\n          v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\n          type=\"submit\"\n          variant=\"primary\"\n          class=\"mr-1\"\n          :disabled=\"isSubmitting\"\n          @click=\"Submit\"\n        >\n          <b-spinner v-if=\"isSubmitting\" small />\n          <span v-if=\"isSubmitting\">Please Wait</span>\n\n          <span v-if=\"!isSubmitting\">Submit</span>\n        </b-button>\n      </b-col>\n    </b-row>\n  </b-form>\n</template>\n\n<script>\nimport {\n  BRow,\n  BCol,\n  BFormGroup,\n  BFormInput,\n  BFormCheckbox,\n  BForm,\n  BButton,\n  BFormDatepicker,\n  BFormTimepicker,\n  BCard,\n  BSpinner,\n  BFormFile,\n  BFormTextarea,\n  BInputGroupAppend,\n  BInputGroup,\n} from \"bootstrap-vue\";\nimport Ripple from \"vue-ripple-directive\";\nimport vSelect from \"vue-select\";\nimport axios from \"@axios\";\nimport { heightTransition } from \"@core/mixins/ui/transition\";\nimport ToastificationContent from \"@core/components/toastification/ToastificationContent.vue\";\nimport { VueAutosuggest } from \"vue-autosuggest\";\n\nimport Cleave from \"vue-cleave-component\";\nimport \"cleave.js/dist/addons/cleave-phone.us\";\n\nexport default {\n  components: {\n    BRow,\n    BCol,\n    BFormGroup,\n    BFormInput,\n    BFormCheckbox,\n    BForm,\n    BButton,\n    BFormDatepicker,\n    vSelect,\n    VueAutosuggest,\n    BCard,\n    BSpinner,\n    BFormFile,\n    BFormTextarea,\n    BInputGroupAppend,\n    Cleave,\n    BInputGroup,\n    BFormTimepicker,\n  },\n  directives: {\n    Ripple,\n  },\n\n  mixins: [heightTransition],\n\n  data() {\n    return {\n      options: {\n        date: {\n          date: true,\n          delimiter: \"-\",\n          datePattern: [\"Y\", \"m\", \"d\"],\n        },\n      },\n\n      isSubmitting: false,\n\n      formValues: {\n        name: null,\n        type:null,\n        issue:null,\n      },\n      filteredOptions: [],\n      userList: [],\n        RequestType: [\n         { value: \"Material\", text: \"Material\" },\n        { value: \"Repair\", text: \"Repair\" },\n        { value: \"Salary Issue\", text: \"Salary Issue\" },\n        { value: \"Iqama Issue\", text: \"Iqama Issue\" },\n      ],\n    };\n  },\n  mounted() {\n    this.initTrHeight();\n  },\n  created() {\n    window.addEventListener(\"resize\", this.initTrHeight);\n  },\n  destroyed() {\n    window.removeEventListener(\"resize\", this.initTrHeight);\n  },\n  methods: {\n    Submit() {\n      this.isSubmitting = true;\n      console.log(\"submt\", this.formValues);\n\n      // create new formData for post request to send files\n\n      const formData = new FormData();\n\n      Object.entries(this.formValues).forEach(([key, value]) => {\n        if (\n          typeof value === \"object\" &&\n          value !== null &&\n          !Array.isArray(value)\n        ) {\n          if (value instanceof File) {\n            formData.append(key, value);\n          } else {\n            formData.append(key, JSON.stringify(value));\n          }\n        } else if (\n          typeof value === \"object\" &&\n          value !== null &&\n          Array.isArray(value)\n        ) {\n          formData.append(key, JSON.stringify(value));\n        } else if (value !== null) {\n          formData.append(key, value);\n        }\n      });\n\n      axios\n        .post(\"/addRequestCenter\", formData)\n        .then((response) => {\n          if (response.data.hasOwnProperty(\"success\")) {\n            if (response.data.success === true) {\n              console.log(response.data.data);\n\n              this.$router.replace(\"/addRequestCenter\").then(() => {\n                this.$toast({\n                  component: ToastificationContent,\n                  position: \"top-right\",\n                  props: {\n                    title: response.data.message,\n                    icon: \"EditIcon\",\n                    variant: \"success\",\n                  },\n                });\n              });\n            } else {\n              this.isSubmitting = false;\n\n              this.$toast({\n                component: ToastificationContent,\n                position: \"top-right\",\n                props: {\n                  title: response.data.message,\n                  icon: \"AlertCircleIcon\",\n                  variant: \"danger\",\n                },\n              });\n            }\n          } else {\n            this.isSubmitting = false;\n\n            this.$toast({\n              component: ToastificationContent,\n              position: \"top-right\",\n              props: {\n                title: \"Error\",\n                icon: \"AlertCircleIcon\",\n                variant: \"danger\",\n                text: \"Something went wrong, try again later\",\n              },\n            });\n          }\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },   \n    assignOwnerField(event) {\n      console.log(\"name\", event.item);\n      this.formValues.name = event.item.name;\n      this.formValues.name_id = event.item.id;\n\n      return;\n    },\n    initTrHeight() {\n      this.trSetHeight(null);\n      this.$nextTick(() => {\n        // this.trSetHeight(this.$refs.form.scrollHeight);\n      });\n    },\n  },\n};\n</script>\n\n<style>\ntextarea {\n  resize: vertical;\n}\ninput[type=\"number\"]::-webkit-inner-spin-button,\ninput[type=\"number\"]::-webkit-outer-spin-button {\n  -webkit-appearance: none;\n  margin: 0;\n}\n.hr_divider {\n  border-bottom: 2px solid #82868b;\n}\n</style>\n<style lang=\"scss\">\n@import \"@core/scss/vue/libs/vue-autosuggest.scss\";\n</style>"],"sourceRoot":"src/views/table/bs-table/RequestCenter"}]}