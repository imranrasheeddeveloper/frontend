A,OAAA,EAAA,OAPA;AAQA,MAAA,WAAA,EAAA,WARA;AASA,MAAA,UAAA,EAAA,UATA;AAUA,MAAA,QAAA,EAAA,QAVA;AAWA,MAAA,cAAA,EAAA,cAXA;AAYA,MAAA,WAAA,EAAA,WAZA;AAaA,MAAA,MAAA,EAAA,MAbA;AAcA,MAAA,aAAA,EAAA,aAdA;AAeA,MAAA,gBAAA,EAAA,gBAfA;AAgBA,MAAA,WAAA,EAAA,WAhBA;AAkBA;AACA,MAAA,UAAA,EAAA,UAnBA;AAqBA;AACA,MAAA,sBAAA,EAAA,sBAtBA;AAuBA,MAAA,mBAAA,EAAA,mBAvBA;AAwBA,MAAA,wBAAA,EAAA,wBAxBA;AA0BA,MAAA,WAAA,EAAA,WA1BA;AA2BA,MAAA,WAAA,EAAA,WA3BA;AA4BA,MAAA,aAAA,EAAA,aA5BA;AA8BA;AACA,MAAA,UAAA,EAAA,UA/BA;AAgCA,MAAA,UAAA,EAAA,UAhCA;AAiCA,MAAA,YAAA,EAAA;AAjCA,KAAA;AAmCA;AAnHA,CAAA","sourcesContent":["<template>\n\n  <div>\n\n    <user-list-add-new\n      :is-add-new-user-sidebar-active.sync=\"isAddNewUserSidebarActive\"\n      :role-options=\"roleOptions\"\n      :plan-options=\"planOptions\"\n      @refetch-data=\"refetchData\"\n    />\n\n    <!-- Filters -->\n    <users-list-filters\n      :role-filter.sync=\"roleFilter\"\n      :plan-filter.sync=\"planFilter\"\n      :status-filter.sync=\"statusFilter\"\n      :role-options=\"roleOptions\"\n      :plan-options=\"planOptions\"\n      :status-options=\"statusOptions\"\n    />\n\n    <!-- Table Container Card -->\n    <b-card\n      no-body\n      class=\"mb-0\"\n    >\n\n      <div class=\"m-2\">\n\n        <!-- Table Top -->\n        <b-row>\n\n          <!-- Per Page -->\n          <b-col\n            cols=\"12\"\n            md=\"6\"\n            class=\"d-flex align-items-center justify-content-start mb-1 mb-md-0\"\n          >\n            <label>Show</label>\n            <v-select\n              v-model=\"perPage\"\n              :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\n              :options=\"perPageOptions\"\n              :clearable=\"false\"\n              class=\"per-page-selector d-inline-block mx-50\"\n            />\n            <label>entries</label>\n          </b-col>\n\n          <!-- Search -->\n          <b-col\n            cols=\"12\"\n            md=\"6\"\n          >\n            <div class=\"d-flex align-items-center justify-content-end\">\n              <b-form-input\n                v-model=\"searchQuery\"\n                class=\"d-inline-block mr-1\"\n                placeholder=\"Search...\"\n              />\n              <b-button\n                variant=\"primary\"\n                @click=\"isAddNewUserSidebarActive = true\"\n              >\n                <span class=\"text-nowrap\">Add User</span>\n              </b-button>\n            </div>\n          </b-col>\n        </b-row>\n\n      </div>\n\n      <b-table\n        ref=\"refUserListTable\"\n        class=\"position-relative\"\n        :items=\"fetchUsers\"\n        responsive\n        :fields=\"tableColumns\"\n        primary-key=\"id\"\n        :sort-by.sync=\"sortBy\"\n        show-empty\n        empty-text=\"No matching records found\"\n        :sort-desc.sync=\"isSortDirDesc\"\n      >\n\n        <!-- Column: User -->\n        <template #cell(user)=\"data\">\n          <b-media vertical-align=\"center\">\n            <template #aside>\n              <b-avatar\n                size=\"32\"\n                :src=\"data.item.avatar\"\n                :text=\"avatarText(data.item.fullName)\"\n                :variant=\"`light-${resolveUserRoleVariant(data.item.role)}`\"\n                :to=\"{ name: 'apps-users-view', params: { id: data.item.id } }\"\n              />\n            </template>\n            <b-link\n              :to=\"{ name: 'apps-users-view', params: { id: data.item.id } }\"\n              class=\"font-weight-bold d-block text-nowrap\"\n            >\n              {{ data.item.fullName }}\n            </b-link>\n            <small class=\"text-muted\">@{{ data.item.username }}</small>\n          </b-media>\n        </template>\n\n        <!-- Column: Role -->\n        <template #cell(role)=\"data\">\n          <div class=\"text-nowrap\">\n            <feather-icon\n              :icon=\"resolveUserRoleIcon(data.item.role)\"\n              size=\"18\"\n              class=\"mr-50\"\n              :class=\"`text-${resolveUserRoleVariant(data.item.role)}`\"\n            />\n            <span class=\"align-text-top text-capitalize\">{{ data.item.role }}</span>\n          </div>\n        </template>\n\n        <!-- Column: Status -->\n        <template #cell(status)=\"data\">\n          <b-badge\n            pill\n            :variant=\"`light-${resolveUserStatusVariant(data.item.status)}`\"\n            class=\"text-capitalize\"\n          >\n            {{ data.item.status }}\n          </b-badge>\n        </template>\n\n        <!-- Column: Actions -->\n        <template #cell(actions)=\"data\">\n          <b-dropdown\n            variant=\"link\"\n            no-caret\n            :right=\"$store.state.appConfig.isRTL\"\n          >\n\n            <template #button-content>\n              <feather-icon\n                icon=\"MoreVerticalIcon\"\n                size=\"16\"\n                class=\"align-middle text-body\"\n              />\n            </template>\n            <b-dropdown-item :to=\"{ name: 'apps-users-view', params: { id: data.item.id } }\">\n              <feather-icon icon=\"FileTextIcon\" />\n              <span class=\"align-middle ml-50\">Details</span>\n            </b-dropdown-item>\n\n            <b-dropdown-item :to=\"{ name: 'apps-users-edit', params: { id: data.item.id } }\">\n              <feather-icon icon=\"EditIcon\" />\n              <span class=\"align-middle ml-50\">Edit</span>\n            </b-dropdown-item>\n\n            <b-dropdown-item>\n              <feather-icon icon=\"TrashIcon\" />\n              <span class=\"align-middle ml-50\">Delete</span>\n            </b-dropdown-item>\n          </b-dropdown>\n        </template>\n\n      </b-table>\n      <div class=\"mx-2 mb-2\">\n        <b-row>\n\n          <b-col\n            cols=\"12\"\n            sm=\"6\"\n            class=\"d-flex align-items-center justify-content-center justify-content-sm-start\"\n          >\n            <span class=\"text-muted\">Showing {{ dataMeta.from }} to {{ dataMeta.to }} of {{ dataMeta.of }} entries</span>\n          </b-col>\n          <!-- Pagination -->\n          <b-col\n            cols=\"12\"\n            sm=\"6\"\n            class=\"d-flex align-items-center justify-content-center justify-content-sm-end\"\n          >\n\n            <b-pagination\n              v-model=\"currentPage\"\n              :total-rows=\"totalUsers\"\n              :per-page=\"perPage\"\n              first-number\n              last-number\n              class=\"mb-0 mt-1 mt-sm-0\"\n              prev-class=\"prev-item\"\n              next-class=\"next-item\"\n            >\n              <template #prev-text>\n                <feather-icon\n                  icon=\"ChevronLeftIcon\"\n                  size=\"18\"\n                />\n              </template>\n              <template #next-text>\n                <feather-icon\n                  icon=\"ChevronRightIcon\"\n                  size=\"18\"\n                />\n              </template>\n            </b-pagination>\n\n          </b-col>\n\n        </b-row>\n      </div>\n    </b-card>\n  </div>\n</template>\n\n<script>\nimport {\n  BCard, BRow, BCol, BFormInput, BButton, BTable, BMedia, BAvatar, BLink,\n  BBadge, BDropdown, BDropdownItem, BPagination,\n} from 'bootstrap-vue'\nimport vSelect from 'vue-select'\nimport store from '@/store'\nimport { ref, onUnmounted } from '@vue/composition-api'\nimport { avatarText } from '@core/utils/filter'\nimport UsersListFilters from './UsersListFilters.vue'\nimport useUsersList from './useUsersList'\nimport userStoreModule from '../userStoreModule'\nimport UserListAddNew from './UserListAddNew.vue'\n\nexport default {\n  components: {\n    UsersListFilters,\n    UserListAddNew,\n\n    BCard,\n    BRow,\n    BCol,\n    BFormInput,\n    BButton,\n    BTable,\n    BMedia,\n    BAvatar,\n    BLink,\n    BBadge,\n    BDropdown,\n    BDropdownItem,\n    BPagination,\n\n    vSelect,\n  },\n  setup() {\n    const USER_APP_STORE_MODULE_NAME = 'app-user'\n\n    // Register module\n    if (!store.hasModule(USER_APP_STORE_MODULE_NAME)) store.registerModule(USER_APP_STORE_MODULE_NAME, userStoreModule)\n\n    // UnRegister on leave\n    onUnmounted(() => {\n      if (store.hasModule(USER_APP_STORE_MODULE_NAME)) store.unregisterModule(USER_APP_STORE_MODULE_NAME)\n    })\n\n    const isAddNewUserSidebarActive = ref(false)\n\n    const roleOptions = [\n      { label: 'Admin', value: 'admin' },\n      { label: 'Author', value: 'author' },\n      { label: 'Editor', value: 'editor' },\n      { label: 'Maintainer', value: 'maintainer' },\n      { label: 'Subscriber', value: 'subscriber' },\n    ]\n\n    const planOptions = [\n      { label: 'Basic', value: 'basic' },\n      { label: 'Company', value: 'company' },\n      { label: 'Enterprise', value: 'enterprise' },\n      { label: 'Team', value: 'team' },\n    ]\n\n    const statusOptions = [\n      { label: 'Pending', value: 'pending' },\n      { label: 'Active', value: 'active' },\n      { label: 'Inactive', value: 'inactive' },\n    ]\n\n    const {\n      fetchUsers,\n      tableColumns,\n      perPage,\n      currentPage,\n      totalUsers,\n      dataMeta,\n      perPageOptions,\n      searchQuery,\n      sortBy,\n      isSortDirDesc,\n      refUserListTable,\n      refetchData,\n\n      // UI\n      resolveUserRoleVariant,\n      resolveUserRoleIcon,\n      resolveUserStatusVariant,\n\n      // Extra Filters\n      roleFilter,\n      planFilter,\n      statusFilter,\n    } = useUsersList()\n\n    return {\n\n      // Sidebar\n      isAddNewUserSidebarActive,\n\n      fetchUsers,\n      tableColumns,\n      perPage,\n      currentPage,\n      totalUsers,\n      dataMeta,\n      perPageOptions,\n      searchQuery,\n      sortBy,\n      isSortDirDesc,\n      refUserListTable,\n      refetchData,\n\n      // Filter\n      avatarText,\n\n      // UI\n      resolveUserRoleVariant,\n      resolveUserRoleIcon,\n      resolveUserStatusVariant,\n\n      roleOptions,\n      planOptions,\n      statusOptions,\n\n      // Extra Filters\n      roleFilter,\n      planFilter,\n      statusFilter,\n    }\n  },\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.per-page-selector {\n  width: 90px;\n}\n</style>\n\n<style lang=\"scss\">\n@import '@core/scss/vue/libs/vue-select.scss';\n</style>\n"],"sourceRoot":"src/views/apps/user/users-list"}]}