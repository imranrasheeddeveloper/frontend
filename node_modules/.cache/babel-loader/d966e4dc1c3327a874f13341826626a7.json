AqBA,EAAA,IArBA,kBAqBA;AACA,WAAA;AACA,MAAA,QAAA,EAAA;AACA,QAAA,YAAA,EAAA;AACA,UAAA,KAAA,EAAA;AACA,YAAA,SAAA,EAAA;AAAA,cAAA,OAAA,EAAA;AAAA,aADA;AAEA,YAAA,OAAA,EAAA;AAAA,cAAA,IAAA,EAAA;AAAA;AAFA,WADA;AAKA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,EAAA,KADA;AAEA,YAAA,OAAA,EAAA;AACA,cAAA,IAAA,EAAA,CADA;AAEA,cAAA,KAAA,EAAA;AAFA;AAFA,WALA;AAYA,UAAA,MAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,cAAA,MAAA,EAAA;AADA;AADA,WAZA;AAiBA,UAAA,MAAA,EAAA,CACA,SADA,EAEA,SAFA,EAGA,YAAA,CAAA,OAHA,EAIA,SAJA,EAKA,SALA,EAMA,SANA,CAjBA;AAyBA,UAAA,WAAA,EAAA;AACA,YAAA,GAAA,EAAA;AACA,cAAA,WAAA,EAAA,KADA;AAEA,cAAA,WAAA,EAAA,IAFA;AAGA,cAAA,WAAA,EAAA;AAHA;AADA,WAzBA;AAgCA,UAAA,OAAA,EAAA;AACA,YAAA,CAAA,EAAA;AAAA,cAAA,IAAA,EAAA;AAAA;AADA,WAhCA;AAmCA,UAAA,KAAA,EAAA;AACA,YAAA,IAAA,EAAA;AADA;AAnCA;AADA;AADA,KAAA;AA2CA,GAjEA;AAkEA,EAAA,OAAA,EAAA;AACA,IAAA,UAAA,EAAA;AADA;AAlEA,CAAA","sourcesContent":["<template>\n  <b-card v-if=\"data\">\n    <b-row class=\"pb-50\">\n\n      <!-- text and button -->\n      <b-col\n        sm=\"6\"\n        order-sm=\"1\"\n        order=\"2\"\n        class=\"d-flex justify-content-between flex-column mt-1 mt-sm-0\"\n      >\n        <div>\n          <h2 class=\"font-weight-bolder mb-25\">\n            {{ kFormatter(data.sessions) }}\n          </h2>\n          <b-card-text class=\"font-weight-bold mb-2\">\n            Avg Sessions\n          </b-card-text>\n\n          <h5 class=\"font-medium-2\">\n            <span class=\"text-success mr-50\">{{ data.growth }}</span>\n            <span>vs last 7 days</span>\n          </h5>\n        </div>\n\n        <b-button\n          v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\n          variant=\"primary\"\n          class=\"shadow\"\n        >\n          <span>View Details </span>\n          <feather-icon icon=\"ChevronsRightIcon\" />\n        </b-button>\n      </b-col>\n\n      <!-- dropdown and chart -->\n      <b-col\n        sm=\"6\"\n        cols=\"12\"\n        order-sm=\"2\"\n        order=\"1\"\n        class=\"d-flex justify-content-between flex-column text-right\"\n      >\n        <div>\n          <b-dropdown\n            text=\"Last 7 Days\"\n            variant=\"transparent\"\n            class=\"chart-dropdown\"\n            left\n            no-caret\n            size=\"sm\"\n          >\n            <b-dropdown-item\n              v-for=\"day in data.lastDays\"\n              :key=\"day\"\n            >\n              {{ day }}\n            </b-dropdown-item>\n          </b-dropdown>\n        </div>\n\n        <!-- apex chart -->\n        <vue-apex-charts\n          type=\"bar\"\n          height=\"200\"\n          :options=\"salesBar.chartOptions\"\n          :series=\"data.salesBar.series\"\n        />\n      </b-col>\n    </b-row>\n    <hr>\n\n    <!-- progress bar -->\n    <b-row class=\"avg-sessions pt-50\">\n      <b-col\n        cols=\"6\"\n        class=\"mb-2\"\n      >\n        <b-card-text class=\"mb-50\">\n          Goal: ${{ data.goal }}\n        </b-card-text>\n        <b-progress\n          value=\"50\"\n          max=\"100\"\n          height=\"6px\"\n        />\n      </b-col>\n      <b-col\n        cols=\"6\"\n        class=\"mb-2\"\n      >\n        <b-card-text class=\"mb-50\">\n          Users: {{ kFormatter(data.users) }}\n        </b-card-text>\n        <b-progress\n          value=\"60\"\n          max=\"100\"\n          height=\"6px\"\n          variant=\"warning\"\n        />\n      </b-col>\n      <b-col cols=\"6\">\n        <b-card-text class=\"mb-50\">\n          Retention: {{ data.retention }}%\n        </b-card-text>\n        <b-progress\n          value=\"70\"\n          max=\"100\"\n          height=\"6px\"\n          variant=\"danger\"\n          class=\"mt-25\"\n        />\n      </b-col>\n      <b-col cols=\"6\">\n        <b-card-text class=\"mb-50\">\n          Duration: {{ data.duration }}yr\n        </b-card-text>\n        <b-progress\n          value=\"90\"\n          max=\"100\"\n          variant=\"success\"\n          height=\"6px\"\n          class=\"mt-25\"\n        />\n      </b-col>\n    </b-row>\n  </b-card>\n</template>\n\n<script>\nimport {\n  BCard, BRow, BCol, BButton, BDropdown, BDropdownItem, BProgress, BCardText,\n} from 'bootstrap-vue'\nimport VueApexCharts from 'vue-apexcharts'\nimport Ripple from 'vue-ripple-directive'\nimport { $themeColors } from '@themeConfig'\nimport { kFormatter } from '@core/utils/filter'\n\nexport default {\n  components: {\n    VueApexCharts,\n    BCard,\n    BRow,\n    BCol,\n    BButton,\n    BCardText,\n    BDropdown,\n    BDropdownItem,\n    BProgress,\n  },\n  directives: {\n    Ripple,\n  },\n  props: {\n    data: {\n      type: Object,\n      default: () => {},\n    },\n  },\n  data() {\n    return {\n      salesBar: {\n        chartOptions: {\n          chart: {\n            sparkline: { enabled: true },\n            toolbar: { show: false },\n          },\n          grid: {\n            show: false,\n            padding: {\n              left: 0,\n              right: 0,\n            },\n          },\n          states: {\n            hover: {\n              filter: 'none',\n            },\n          },\n          colors: [\n            '#ebf0f7',\n            '#ebf0f7',\n            $themeColors.primary,\n            '#ebf0f7',\n            '#ebf0f7',\n            '#ebf0f7',\n          ],\n          plotOptions: {\n            bar: {\n              columnWidth: '45%',\n              distributed: true,\n              endingShape: 'rounded',\n            },\n          },\n          tooltip: {\n            x: { show: false },\n          },\n          xaxis: {\n            type: 'numeric',\n          },\n        },\n      },\n    }\n  },\n  methods: {\n    kFormatter,\n  },\n}\n</script>\n"],"sourceRoot":"src/views/dashboard/analytics"}]}