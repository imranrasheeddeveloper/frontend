AAA,IAAA,EAAA,IALA;AAMA,IAAA,IAAA,EAAA,IANA;AAOA,IAAA,SAAA,EAAA,SAPA;AAQA,IAAA,KAAA,EAAA,KARA;AASA,IAAA,OAAA,EAAA,OATA;AAUA,IAAA,SAAA,EAAA,SAVA;AAWA,IAAA,aAAA,EAAA,aAXA;AAYA,IAAA,MAAA,EAAA,MAZA;AAcA;AACA,IAAA,mCAAA,EAAA,mCAfA;AAgBA,IAAA,sCAAA,EAAA;AAhBA,GAJA;AAsBA,EAAA,KAtBA,mBAsBA;AACA,0BAAA,cAAA,EAAA;AAAA,QAAA,qBAAA,mBAAA,qBAAA;AAAA,QAAA,uBAAA,mBAAA,uBAAA;;AAEA,QAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAHA,CAKA;;AACA,QAAA,YAAA,GAAA,SAAA,YAAA,GAAA;AACA;AACA,uBAAA,SAAA,EAAA;AAAA,UAAA,KAAA,cAAA,KAAA;;AACA,UAAA,WAAA,GAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA,IAAA;AACA,UAAA,SAAA,GAAA,WAAA,CAAA,SAAA,CAAA,WAAA,CAAA,WAAA,CAAA,GAAA,IAAA,CAAA,CAAA;AAEA,MAAA,KAAA,CAAA,QAAA,CAAA,4BAAA,EAAA;AAAA,QAAA,SAAA,EAAA;AAAA,OAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,CAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,KAAA,GAAA,SAAA;AACA;AACA,OARA;AASA,KAfA,CANA,CAuBA;;;AACA,QAAA,aAAA,GAAA,GAAA,CAAA,IAAA,CAAA;AAEA,IAAA,YAAA;AAEA,WAAA;AAEA;AACA,MAAA,OAAA,EAAA,OAHA;AAKA;AACA,MAAA,aAAA,EAAA,aANA;AAOA,MAAA,qBAAA,EAAA,qBAPA;AAQA,MAAA,uBAAA,EAAA;AARA,KAAA;AAUA;AA5DA,CAAA","sourcesContent":["<template>\n  <section class=\"app-ecommerce-details\">\n\n    <!-- Alert: No item found -->\n    <b-alert\n      variant=\"danger\"\n      :show=\"product === undefined\"\n    >\n      <h4 class=\"alert-heading\">\n        Error fetching product data\n      </h4>\n      <div class=\"alert-body\">\n        No item found with this item slug. Check\n        <b-link\n          class=\"alert-link\"\n          :to=\"{ name: 'apps-e-commerce-shop'}\"\n        >\n          Shop\n        </b-link>\n        for other items.\n      </div>\n    </b-alert>\n\n    <!-- Content -->\n    <b-card\n      v-if=\"product\"\n      no-body\n    >\n      <b-card-body>\n        <b-row class=\"my-2\">\n\n          <!-- Left: Product Image Container -->\n          <b-col\n            cols=\"12\"\n            md=\"5\"\n            class=\"d-flex align-items-center justify-content-center mb-2 mb-md-0\"\n          >\n            <div class=\"d-flex align-items-center justify-content-center\">\n              <b-img\n                :src=\"product.image\"\n                :alt=\"`Image of ${product.name}`\"\n                class=\"product-img\"\n                fluid\n              />\n            </div>\n          </b-col>\n\n          <!-- Right: Product Details -->\n          <b-col\n            cols=\"12\"\n            md=\"7\"\n          >\n\n            <!-- Product Name -->\n            <h4>{{ product.name }}</h4>\n\n            <!-- Product Brand -->\n            <b-card-text class=\"item-company mb-0\">\n              <span>by</span>\n              <b-link class=\"company-name\">\n                {{ product.brand }}\n              </b-link>\n            </b-card-text>\n\n            <!-- Price And Ratings -->\n            <div class=\"ecommerce-details-price d-flex flex-wrap mt-1\">\n              <h4 class=\"item-price mr-1\">\n                ${{ product.price }}\n              </h4>\n              <ul class=\"unstyled-list list-inline pl-1 border-left\">\n                <li\n                  v-for=\"star in 5\"\n                  :key=\"star\"\n                  class=\"ratings-list-item mr-25\"\n                >\n                  <feather-icon\n                    icon=\"StarIcon\"\n                    size=\"18\"\n                    :class=\"[{'fill-current': star <= product.rating}, star <= product.rating ? 'text-warning' : 'text-muted']\"\n                  />\n                </li>\n              </ul>\n            </div>\n\n            <!-- Avability -->\n            <b-card-text>Available - <span class=\"text-success\">In stock</span></b-card-text>\n\n            <!-- Product Description -->\n            <b-card-text>{{ product.description }}</b-card-text>\n\n            <!-- Product Meta [Free shpping, EMI, etc.] -->\n            <ul class=\"product-features list-unstyled\">\n              <li v-if=\"product.hasFreeShipping\">\n                <feather-icon icon=\"ShoppingCartIcon\" />\n                <span>Free Shipping</span></li>\n              <li>\n                <feather-icon icon=\"DollarSignIcon\" />\n                <span>EMI options available</span>\n              </li>\n            </ul>\n\n            <hr>\n\n            <!-- Colors -->\n            <div class=\"product-color-options\">\n              <h6>Colors</h6>\n              <ul class=\"list-unstyled mb-0\">\n                <li\n                  v-for=\"color in product.colorOptions\"\n                  :key=\"color\"\n                  class=\"d-inline-block\"\n                  :class=\"{'selected': selectedColor === color}\"\n                  @click=\"selectedColor = color\"\n                >\n                  <div\n                    class=\"color-option\"\n                    :class=\"`b-${color}`\"\n                  >\n                    <div\n                      class=\"filloption\"\n                      :class=\"`bg-${color}`\"\n                    />\n                  </div>\n                </li>\n              </ul>\n            </div>\n\n            <hr>\n\n            <div class=\"d-flex flex-column flex-sm-row pt-1\">\n              <b-button\n                v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\n                variant=\"primary\"\n                class=\"btn-cart mr-0 mr-sm-1 mb-1 mb-sm-0\"\n                @click=\"handleCartActionClick(product)\"\n              >\n                <feather-icon\n                  icon=\"ShoppingCartIcon\"\n                  class=\"mr-50\"\n                />\n                <span>{{ product.isInCart ? 'View In Cart' : 'Add to Cart' }}</span>\n              </b-button>\n              <b-button\n                variant=\"outline-secondary\"\n                class=\"btn-wishlist mr-0 mr-sm-1 mb-1 mb-sm-0\"\n                @click=\"toggleProductInWishlist(product)\"\n              >\n                <feather-icon\n                  icon=\"HeartIcon\"\n                  class=\"mr-50\"\n                  :class=\"{'text-danger': product.isInWishlist}\"\n                />\n                <span>Wishlist</span>\n              </b-button>\n              <b-dropdown\n                variant=\"outline-secondary\"\n                no-caret\n                toggle-class=\"btn-icon\"\n                class=\"btn-share\"\n                right\n              >\n                <template #button-content>\n                  <feather-icon icon=\"Share2Icon\" />\n                </template>\n                <b-dropdown-item\n                  v-for=\"icon in [ 'FacebookIcon', 'TwitterIcon', 'YoutubeIcon', 'InstagramIcon', ]\"\n                  :key=\"icon\"\n                >\n                  <feather-icon :icon=\"icon\" />\n                </b-dropdown-item>\n              </b-dropdown>\n            </div>\n          </b-col>\n        </b-row>\n      </b-card-body>\n\n      <!-- Static Content -->\n      <e-commerce-product-details-item-features />\n\n      <!-- Static Content -->\n      <!-- Slider: Related Products -->\n      <e-commerce-product-details-related-products />\n    </b-card>\n  </section>\n</template>\n\n<script>\nimport { useRouter } from '@core/utils/utils'\nimport store from '@/store'\nimport { ref } from '@vue/composition-api'\nimport {\n  BCard, BCardBody, BRow, BCol, BImg, BCardText, BLink, BButton, BDropdown, BDropdownItem, BAlert,\n} from 'bootstrap-vue'\nimport Ripple from 'vue-ripple-directive'\nimport ECommerceProductDetailsItemFeatures from './ECommerceProductDetailsItemFeatures.vue'\nimport ECommerceProductDetailsRelatedProducts from './ECommerceProductDetailsRelatedProducts.vue'\nimport { useEcommerceUi } from '../useEcommerce'\n\nexport default {\n  directives: {\n    Ripple,\n  },\n  components: {\n    // BSV\n    BCard,\n    BCardBody,\n    BRow,\n    BCol,\n    BImg,\n    BCardText,\n    BLink,\n    BButton,\n    BDropdown,\n    BDropdownItem,\n    BAlert,\n\n    // SFC\n    ECommerceProductDetailsItemFeatures,\n    ECommerceProductDetailsRelatedProducts,\n  },\n  setup() {\n    const { handleCartActionClick, toggleProductInWishlist } = useEcommerceUi()\n\n    const product = ref(null)\n\n    // Remote Data\n    const fetchProduct = () => {\n      // Get product  id from URL\n      const { route } = useRouter()\n      const productSlug = route.value.params.slug\n      const productId = productSlug.substring(productSlug.lastIndexOf('-') + 1)\n\n      store.dispatch('app-ecommerce/fetchProduct', { productId })\n        .then(response => {\n          product.value = response.data.product\n        })\n        .catch(error => {\n          if (error.response.status === 404) {\n            product.value = undefined\n          }\n        })\n    }\n\n    // UI\n    const selectedColor = ref(null)\n\n    fetchProduct()\n\n    return {\n\n      // Fetched Product\n      product,\n\n      // UI\n      selectedColor,\n      handleCartActionClick,\n      toggleProductInWishlist,\n    }\n  },\n}\n</script>\n\n<style lang=\"scss\">\n@import \"~@core/scss/base/pages/app-ecommerce-details.scss\";\n</style>\n"],"sourceRoot":"src/views/apps/e-commerce/e-commerce-product-details"}]}