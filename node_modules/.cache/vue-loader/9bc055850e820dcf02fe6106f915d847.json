{"remainingRequest":"C:\\xampp\\htdocs\\GulflamarERPSystem\\frontend\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\xampp\\htdocs\\GulflamarERPSystem\\frontend\\src\\views\\table\\bs-table\\client\\clientTableView.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"C:\\xampp\\htdocs\\GulflamarERPSystem\\frontend\\src\\views\\table\\bs-table\\client\\clientTableView.vue","mtime":1659017882734},{"path":"C:\\xampp\\htdocs\\GulflamarERPSystem\\frontend\\node_modules\\css-loader\\dist\\cjs.js","mtime":1658911433225},{"path":"C:\\xampp\\htdocs\\GulflamarERPSystem\\frontend\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1658911433727},{"path":"C:\\xampp\\htdocs\\GulflamarERPSystem\\frontend\\node_modules\\postcss-loader\\src\\index.js","mtime":1658911433648},{"path":"C:\\xampp\\htdocs\\GulflamarERPSystem\\frontend\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1658911406787},{"path":"C:\\xampp\\htdocs\\GulflamarERPSystem\\frontend\\node_modules\\vue-loader\\lib\\index.js","mtime":1658911433727}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["clientTableView.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAutCA;AACA;;AAEA;AACA;;AAEA;;AAEA;AACA;AACA","file":"clientTableView.vue","sourceRoot":"src/views/table/bs-table/client","sourcesContent":["<template>\n\n  <div>\n\n    <!-- search input -->\n    <div class=\"custom-search d-flex justify-content-between\">\n      <b-form-group>\n        <div class=\"d-flex align-items-center\">\n          <!-- <label class=\"mr-1\">Search</label> -->\n\n          <b-input-group>\n            <b-input-group-prepend>\n              <b-button\n                variant=\"outline-primary\"\n                @click=\"page=1, getEmployees()\"\n              >\n                <feather-icon icon=\"SearchIcon\" />\n              </b-button>\n            </b-input-group-prepend>\n            <b-form-input\n              v-model=\"searchTerm\"\n              placeholder=\"Search\"\n              type=\"text\"\n              class=\" d-inline-block\"\n              @keyup=\"searchTimeOut()\"\n            />\n            <!-- <b-input-group-append>\n                <b-button\n                :to=\"{ name: 'create-new-client'}\"\n                v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\n                variant=\"outline-primary\">\n                  ADD +\n                </b-button>\n              </b-input-group-append> -->\n            <!--  -->\n          </b-input-group>\n\n        </div>\n      </b-form-group>\n\n      <b-form-group v-if=\"$ability.can('add/copy', 'client')\">\n        <div class=\"d-flex align-items-center\">\n\n          <b-input-group>\n\n            <b-input-group-prepend>\n\n              <div id=\"my-container\">\n\n                <b-button\n                  id=\"popover-reactive-1\"\n                  ref=\"button\"\n                  v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\n                  title=\"Export\"\n                  variant=\"outline-primary\"\n                >\n                  <feather-icon icon=\"DownloadIcon\" />\n                </b-button>\n\n                <b-popover\n                  ref=\"popover\"\n                  target=\"popover-reactive-1\"\n                  triggers=\"click\"\n                  :show.sync=\"popoverShow\"\n                  placement=\"auto\"\n                  container=\"my-container\"\n                >\n                  <template #title>\n                    <div class=\"d-flex justify-content-between align-items-center\">\n                      <span>Export Table</span>\n                    </div>\n                  </template>\n\n                  <div>\n                    <b-form-group\n                      label=\"Search Filter\"\n                    >\n                      <b-form-input\n                        v-model=\"searchTerm\"\n                        placeholder=\"\"\n                        size=\"sm\"\n                      />\n                    </b-form-group>\n\n                    <b-form-group\n                      label=\"Records Limit\"\n                    >\n                      <b-form-input\n                        v-model=\"exportLimit\"\n                        type=\"text\"\n                        placeholder=\"\"\n                        size=\"sm\"\n                      />\n                    </b-form-group>\n\n                    <!-- button -->\n                    <b-row>\n                      <b-col cols=\"6\">\n                        <b-button\n                          v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\n                          size=\"sm\"\n                          variant=\"danger\"\n                          class=\"mr-1\"\n                          @click=\"onClose\"\n                        >\n                          <feather-icon icon=\"XIcon\" />\n                        </b-button>\n                      </b-col>\n\n                      <b-col cols=\"6\">\n                        <b-button\n                          v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\n                          size=\"sm\"\n                          variant=\"primary\"\n                          :href=\"'/api/clientCsv?searchTerm='+ searchTerm+' &amp;limit='+exportLimit\"\n                          target=\"_blank\"\n                        >\n                          <feather-icon icon=\"DownloadIcon\" />\n                        </b-button>\n                      </b-col>\n                    </b-row>\n                  </div>\n                </b-popover>\n\n              </div>\n\n            </b-input-group-prepend>\n\n            <b-input-group-prepend>\n\n              <b-button\n                v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\n                variant=\"primary\"\n\n                :to=\"{ name: 'create-new-client'}\"\n\n                title=\"Add Client\"\n              >\n                ADD+\n              </b-button>\n            </b-input-group-prepend>\n\n          </b-input-group>\n\n        </div>\n      </b-form-group>\n    </div>\n\n    <b-table\n      :items=\"items\"\n      :fields=\"fields\"\n      responsive\n      class=\"mb-0 bg-white\"\n      :busy=\"isBusy\"\n    >\n      <template #cell(Options)=\"row\">\n        <!-- <div> -->\n          <!-- As `row.showDetails` is one-way, we call the toggleDetails function on @change -->\n          <!-- <b-form-checkbox\n            v-model=\"row.detailsShowing\"\n            @change=\"row.toggleDetails\"\n          >\n            {{ row.detailsShowing ? 'Hide' : 'View Details' }}\n          </b-form-checkbox>\n          <div class=\"d-flex justify-content-start\">\n            <b-button\n              v-if=\"$ability.can('edit', 'client')\"\n              v-ripple.400=\"'rgba(40, 199, 111, 0.15)'\"\n              :to=\"{ name: 'edit-client', params: { client_code: row.item.client_code }}\"\n              variant=\"flat-success\"\n              class=\"btn-icon rounded-circle\"\n            >\n              <feather-icon icon=\"EditIcon\" />\n            </b-button>\n\n            <b-button\n              v-if=\"$ability.can('delete', 'client')\"\n              v-ripple.400=\"'rgba(40, 199, 111, 0.15)'\"\n              variant=\"flat-danger\"\n              class=\"btn-icon rounded-circle\"\n              @click=\"deleteEmployee(row.item.client_code)\"\n            >\n              <feather-icon icon=\"Trash2Icon\" />\n            </b-button>\n          </div> -->\n        <!-- </div> -->\n\n\n\n        <div>\n        <!-- As `row.showDetails` is one-way, we call the toggleDetails function on @change -->\n        <b-form-group>\n          <div class=\"d-flex align-items-center w-fit-content\">\n            <b-input-group>\n              <b-input-group-prepend>\n                <b-button\n                  size=\"sm\"\n                  v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\n                  variant=\"outline-primary\"\n                  v-bind:class=\"row.detailsShowing ? 'active' : ''\"\n                  @click=\"row.toggleDetails\"\n                >\n                  <feather-icon\n                    icon=\"ChevronUpIcon\"\n                    v-if=\"row.detailsShowing\"\n                  />\n\n                  <feather-icon icon=\"ChevronDownIcon\" v-else />\n                </b-button>\n              </b-input-group-prepend>\n\n              <b-input-group-append v-if=\"$ability.can('edit', 'client')\">\n                <b-button\n                  size=\"sm\"\n                  :to=\"{ name: 'edit-client', params: {client_code: row.item.client_code } }\"\n                  v-ripple.400=\"'rgba(40, 199, 111, 0.15)'\"\n                  variant=\"outline-primary text-success\"\n                >\n                  <feather-icon icon=\"EditIcon\" />\n                </b-button>\n              </b-input-group-append>\n              <b-input-group-append\n                v-if=\"$ability.can('delete', 'client')\"\n              >\n                <b-button\n                  size=\"sm\"\n                  v-ripple.400=\"'rgba(40, 199, 111, 0.15)'\"\n                  variant=\"outline-primary text-danger\"\n                  @click=\"deleteEmployee(row.item.client_code)\"\n                >\n                  <feather-icon icon=\"Trash2Icon\" />\n                </b-button>\n              </b-input-group-append>\n            </b-input-group>\n          </div>\n        </b-form-group>  \n        </div>\n      </template>\n\n      <!-- full detail on click -->\n      <template #row-details=\"row\">\n        <b-card no-body>\n          <b-row>\n            <b-col cols=\"12\">\n              <b-tabs\n                pills\n                card\n                horizontal\n              >\n\n                <b-tab title=\"Client Details\">\n                  <b-card-text>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Client Name</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.client_name }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Client Name (ar)</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.client_name_ar }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Client Code</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.client_code }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Address</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.address }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>CR Number</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.cr_no }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>VAT Number</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.vat_no }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom  \">\n                      <b-col cols=\"2\">\n                        <strong>CR Attachment</strong>\n                      </b-col>\n                      <b-col\n                        v-if=\"isPdf(row.item.cr)\"\n                        cols=\"10\"\n                      >\n\n                        <a\n                          class=\"btn btn-primary\"\n                          :href=\"row.item.cr\"\n                          target=\"_blank\"\n                        >View file</a>\n\n                      </b-col>\n                      <b-col\n                        v-else\n                        cols=\"10\"\n                      >\n                        <enlargeable-image\n                          :src=\"row.item.cr\"\n                          :src_large=\"row.item.cr\"\n                          animation_duration=\"600\"\n                        >\n                          <b-img\n                            style=\"max-height: 80px;\"\n                            thumbnail\n                            :src=\"row.item.cr\"\n                          />\n                        </enlargeable-image>\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom  \">\n                      <b-col cols=\"2\">\n                        <strong>VAT Attachment</strong>\n                      </b-col>\n                      <b-col\n                        v-if=\"isPdf(row.item.vat_cert)\"\n                        cols=\"10\"\n                      >\n\n                        <a\n                          class=\"btn btn-primary\"\n                          :href=\"row.item.vat_cert\"\n                          target=\"_blank\"\n                        >View file</a>\n\n                      </b-col>\n                      <b-col\n                        v-else\n                        cols=\"10\"\n                      >\n                        <enlargeable-image\n                          :src=\"row.item.vat_cert\"\n                          :src_large=\"row.item.vat_cert\"\n                          animation_duration=\"600\"\n                        >\n                          <b-img\n                            style=\"max-height: 80px;\"\n                            thumbnail\n                            :src=\"row.item.vat_cert\"\n                          />\n                        </enlargeable-image>\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>AJEER License</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.ajeer_license }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Department</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.dept_en }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Department (ar)</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.dept_ar }}\n                      </b-col>\n                    </b-row>\n\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Contact Name</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.contactName }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Contact Number</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.contactNo }}\n                      </b-col>\n                    </b-row>\n\n                    <b-row class=\"border-bottom  \">\n                      <b-col cols=\"2\">\n                        <strong>Contract File</strong>\n                      </b-col>\n                      <b-col\n                        v-if=\"isPdf(row.item.contract)\"\n                        cols=\"10\"\n                      >\n\n                        <a\n                          class=\"btn btn-primary\"\n                          :href=\"row.item.contract\"\n                          target=\"_blank\"\n                        >View file</a>\n\n                      </b-col>\n                      <b-col\n                        v-else\n                        cols=\"10\"\n                      >\n                        <enlargeable-image\n                          :src=\"row.item.contract\"\n                          :src_large=\"row.item.contract\"\n                          animation_duration=\"600\"\n                        >\n                          <b-img\n                            style=\"max-height: 80px;\"\n                            thumbnail\n                            :src=\"row.item.contract\"\n                          />\n                        </enlargeable-image>\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Contract Type</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.contract_type }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Contract Status</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.contract_status }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Operational Services</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        <b-list-group horizontal>\n                          <b-list-group-item\n                            v-for=\"oneSkill in row.item.fat_details\"\n                            :key=\"oneSkill\"\n                          >\n                            {{ oneSkill }}\n                          </b-list-group-item>\n\n                        </b-list-group>\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Project Start Date</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.start_date | moment(\"DD/MM/YYYY\") }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Project End Date</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.end_date | moment(\"DD/MM/YYYY\") }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Working Days</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.daysWeek }}\n                      </b-col>\n                    </b-row>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"2\">\n                        <strong>Working Hours</strong>\n                      </b-col>\n                      <b-col cols=\"10\">\n                        {{ row.item.hoursDay }}\n                      </b-col>\n                    </b-row>\n                  </b-card-text>\n                </b-tab>\n\n                <b-tab title=\"Client Services\">\n                  <b-card-text>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"12\">\n                        <b-table\n                          :items=\"row.item.services\"\n                          :fields=\"serviceFields\"\n                          striped\n                          responsive\n                          class=\"mb-0\"\n                          :busy=\"isBusy\"\n                          show-empty\n                        >\n                          <template #empty=\"scope\">\n                            <h4>{{ scope.emptyText }}</h4>\n                          </template>\n\n                          <template #cell(rate)=\"data\">\n\n                            {{ Number( data.value).toLocaleString() }}\n                          </template>\n\n                        </b-table>\n                      </b-col>\n                    </b-row>\n                  </b-card-text>\n                </b-tab>\n\n                <b-tab title=\"Client Location\">\n                  <b-card-text>\n                    <b-row class=\"border-bottom \">\n                      <b-col cols=\"12\">\n                        <b-table\n                          :items=\"row.item.location\"\n                          :fields=\"LocationFields\"\n                          striped\n                          responsive\n                          class=\"mb-0\"\n                          :busy=\"isBusy\"\n                          show-empty\n                        >\n                          <template #empty=\"scope\">\n                            <h4>{{ scope.emptyText }}</h4>\n                          </template>\n\n                          <template #cell(rate)=\"data\">\n\n                            {{ Number( data.value).toLocaleString() }}\n                          </template>\n\n                        </b-table>\n                      </b-col>\n                    </b-row>\n                  </b-card-text>\n                </b-tab>\n\n              </b-tabs>\n            </b-col>\n          </b-row>\n          <b-row class=\"border-bottom \">\n            <b-col cols=\"12\" class=\"text-right\">\n                <strong>Created By : </strong>\n                {{ row.item.created_by }}\n            </b-col>\n          </b-row>\n          <b-row class=\"border-bottom \" v-if=\"row.item.updated_by\">\n            <b-col cols=\"12\" class=\"text-right\">\n                <strong>Updated By : </strong>\n                {{ row.item.updated_by }}\n            </b-col>\n          </b-row>\n\n          <!-- <b-button\n              size=\"sm\"\n              variant=\"outline-secondary\"\n              @click=\"row.toggleDetails\"\n            >\n              Hide Details\n            </b-button> -->\n        </b-card>\n      </template>\n\n      <!-- <template #cell(iqama)=\"data\">\n          <b-avatar :src=\"data.value\" />\n        </template> -->\n\n      <template #cell(client_name)=\"data\">\n\n        {{ data.item.client_name }}\n        </br>\n        {{ data.item.client_name_ar }}\n\n      </template>\n\n      <template #cell(contract_status)=\"data\">\n        <b-badge\n          pill\n          :variant=\"statusVariant(data.value)\"\n        >\n          {{ data.value }}\n        </b-badge>\n      </template>\n\n      <template #cell(passport)=\"data\">\n\n        <enlargeable-image\n          :src=\"data.value\"\n          :src_large=\"data.value\"\n          animation_duration=\"600\"\n        >\n          <b-img\n            thumbnail\n            :src=\"data.value\"\n          />\n        </enlargeable-image>\n      </template>\n\n      <template #cell(passport_2)=\"data\">\n\n        <enlargeable-image\n          :src=\"data.value\"\n          :src_large=\"data.value\"\n          animation_duration=\"600\"\n        >\n          <b-img\n            thumbnail\n            :src=\"data.value\"\n          />\n        </enlargeable-image>\n      </template>\n\n      <template #cell(ajeer)=\"data\">\n\n        <enlargeable-image\n          :src=\"data.value\"\n          :src_large=\"data.value\"\n          animation_duration=\"600\"\n        >\n          <b-img\n            thumbnail\n            :src=\"data.value\"\n          />\n        </enlargeable-image>\n      </template>\n\n      <template #cell(insurance_card)=\"data\">\n\n        <enlargeable-image\n          :src=\"data.value\"\n          :src_large=\"data.value\"\n          animation_duration=\"600\"\n        >\n          <b-img\n            thumbnail\n            :src=\"data.value\"\n          />\n        </enlargeable-image>\n      </template>\n\n      <template #table-busy>\n        <div class=\"text-center text-primary my-2\">\n          <b-spinner class=\"align-middle\" />\n          <strong>Loading...</strong>\n        </div>\n      </template>\n\n      <!-- <template #cell(status)=\"data\">\n          <b-badge :variant=\"status[1][data.value]\">\n            {{ status[0][data.value] }}\n          </b-badge>\n        </template> -->\n    </b-table>\n\n    <div class=\"d-flex justify-content-end\">\n      <b-pagination\n        v-model=\"page\"\n        :total-rows=\"count\"\n        :per-page=\"pageSize\"\n        pills\n        @change=\"handlePageChange\"\n      />\n    </div>\n  </div>\n\n</template>\n\n<script>\nimport BCardCode from '@core/components/b-card-code/BCardCode.vue'\nimport axios from '@axios'\nimport vSelect from 'vue-select'\nimport {\n  BTable, BFormCheckbox, BButton, BCard, BRow, BCol, BAvatar, BBadge, BImg, BTabs, BTab, BCardText, BPagination, BFormGroup, BFormInput, BFormSelect, BDropdown, BDropdownItem, BInputGroup, BInputGroupAppend, BInputGroupPrepend, BFormDatepicker, BFormFile, BFormTextarea, BFormRadio, BForm, BFormCheckboxGroup, BFormRating, BListGroupItem, BListGroup, BPopover, BSpinner,\n} from 'bootstrap-vue'\n\nimport Ripple from 'vue-ripple-directive'\nimport EnlargeableImage from '@diracleo/vue-enlargeable-image'\n\nimport ToastificationContent from '@core/components/toastification/ToastificationContent.vue'\n\nexport default {\n  components: {\n    BCardCode,\n    BTable,\n    BButton,\n    BFormCheckbox,\n    BCard,\n    BRow,\n    BCol,\n    BBadge,\n    BAvatar,\n    BImg,\n    EnlargeableImage,\n    BTabs,\n    BTab,\n    BCardText,\n    BPagination,\n    BFormGroup,\n    BFormInput,\n    BFormSelect,\n    BDropdown,\n    BDropdownItem,\n    BInputGroup,\n    BInputGroupAppe